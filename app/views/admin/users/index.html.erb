<h1>Admin User List</h1>
<table class="table table-striped table-bordered ">
  <thead>
  <td>Parent Email</td>
  <td>Email</td>
  <td>Total Deposit</td>
  <td>Member Total Deposits</td>
  <td>Coins</td>
  <td>Total Used Coins</td>
  <td>Children Members</td>
  <td>Phone Number</td>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.parent_email %></td>
      <td><%= user.email %></td>
      <td><%= user.total_deposit %></td>
      <td><%= user.member_total_deposit %></td>
      <td><%= user.coins %></td>
      <td><%= user.total_used_coins %></td>
      <td><%= user.children_members %></td>
      <td><%= user.phone_number %></td>
    </tr>
    </table>
<% end %>