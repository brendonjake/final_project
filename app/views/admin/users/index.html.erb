<h1>Admin User List</h1>
<table class="table table-striped table-bordered ">
  <thead>
  <td>Parent Email</td>
  <td>Email</td>
  <td>Total Deposit</td>
  <td>Member Total Deposits</td>
  <td>Coins</td>
  <td>Total Used Coins</td>
  <td>Children Members</td>
  <td>Phone Number</td>
  <td>Action</td>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.parent&.email %></td>
      <td><%= user.email %></td>
      <td><%= user.total_deposit %></td>
      <td><%#= user.member_total_deposit %></td>
      <td><%= user.coins %></td>
      <td><%#= user.total_used_coins %></td>
      <td><%= user.children_member %></td>
      <td><%= user.phone %></td>
      <td>
        <%= link_to 'Increase', new_increase_path(user) %>
        <%= link_to 'Deduct', new_deduct_path(user) %>
        <%= link_to 'Bonus', new_bonuse_path(user) %>
      </td>
    </tr>
  <% end %>
</table>