<h1>Client Home</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% if client_user_signed_in? %>
  <h2> Hello <%= current_client_user.email %> </h2>
  <%= link_to 'Sign out', destroy_client_user_session_path, data: { 'turbo-method': :delete } %>
